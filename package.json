{
  "name": "govuk-react",
  "version": "0.1.14",
  "scripts": {
    "start": "start-storybook -p 9009",
    "eslint": "eslint ./src",
    "eslint:fix": "eslint --fix ./src",
    "build": "nwb build-react-component",
    "build:dist": "nwb build-react-app",
    "clean": "rimraf lib && rimraf es",
    "flow": "flow",
    "bundlesize": "bundlesize",
    "test": "CI=true npm run test:unit && npm run eslint && npm run flow",
    "test:unit": "node scripts/test.js --env=jsdom",
    "test:debug": "node  --inspect-brk scripts/test.js --runInBand --env=jsdom",
    "test:snapshot": "npm run test:unit -- -u",
    "coverage": "npm run test:unit -- --coverage",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "prepare": "npm run clean && npm run build",
    "patch": "npm run build && npm run test && npm run build-storybook && npm version patch && npm publish && npm run deploy-storybook",
    "create-component": "node ./scripts/createComponent.js"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!src/**/stories.js",
      "!src/stories/*"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/scripts/babelJest.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "snapshotSerializers": [
      "jest-glamor-react",
      "enzyme-to-json/serializer"
    ]
  },
  "description": "A port of the govuk-frontend components for React using styled-components.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "dependencies": {
    "govuk-colours": "^1.0.3"
  },
  "peerDependencies": {
    "glamorous": ">=4",
    "prop-types": ">=15",
    "react": ">=15"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.9",
    "@storybook/addon-knobs": "^3.3.12",
    "@storybook/addon-links": "^3.3.9",
    "@storybook/react": "^3.3.9",
    "@storybook/storybook-deployer": "^2.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "bundlesize": "^0.16.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-sonar": "0.0.5",
    "eslint-plugin-flowtype": "^2.42.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.5.0",
    "eslint-plugin-react": "^7.5.1",
    "flow-bin": "^0.64.0",
    "glamorous": "^4.11.2",
    "govuk-elements-sass": "^3.1.2",
    "inferno": "^3.10.1",
    "jest": "^22.1.2",
    "jest-glamor-react": "^3.3.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.7.2",
    "normalize.css": "^7.0.0",
    "nwb": "^0.21.2",
    "prettier": "^1.10.2",
    "prop-types": "^15.6.0",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-test-renderer": "^15.6.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "sinon": "^4.2.1",
    "webpack": "^3.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/penx/govuk-react.git"
  },
  "author": "Alasdair McLeay",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/penx/govuk-react/issues"
  },
  "homepage": "https://github.com/penx/govuk-react#readme",
  "bundlesize": [
    {
      "path": "./dist/vendor.*.js",
      "maxSize": "60 kB"
    },
    {
      "path": "./dist/app.*.js",
      "maxSize": "20 kB"
    }
  ]
}
